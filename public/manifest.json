{
  "manifest_version": 3,
  "name": "Hubble Web Clipper",
  "version": "0.0.0",
  "description": "Stop forgetting where you put that dang browser tab",
  "action": {
    "default_title": "Hubble",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [],
  "host_permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
